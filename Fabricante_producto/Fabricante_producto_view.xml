<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="fabricante_en_producto_form_view">
			<field name="name">fabricante_en_producto.form</field>
			<field name="model">product.product</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="product.product_normal_form_view" />
			<field name="arch" type="xml">
				<field name="list_price" position="after">
					<field name="fabricante_id"/>
				</field>
			</field>
		</record>


		<record model="ir.ui.view" id="fabricante_tree_view">
			<field name="name">fabricante.tree</field>
			<field name="model">fabricante</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="">
					<field name="Nombre" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="fabricante_form_view">
			<field name="name">fabricante.form</field>
			<field name="model">fabricante</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="">
					<field name="Nombre" />
				</form>
			</field>
		</record>

	</data>
</openerp>